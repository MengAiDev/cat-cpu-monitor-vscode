{
    "name": "cat-cpu-monitor",
    "displayName": "Cat CPU Monitor",
    "description": "在状态栏显示猫咪动画，速度随CPU使用率变化，点击显示占用最高的进程",
    "version": "1.0.0",
    "publisher": "your-name",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "catCpuMonitor.showProcesses",
                "title": "显示CPU占用最高的进程"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^16.11.7",
        "@types/vscode": "^1.75.0",
        "typescript": "^4.7.4"
    }
}